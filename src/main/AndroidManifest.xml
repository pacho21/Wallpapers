<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pandpdesign.wallpaperslowsdk">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-feature android:name="android.hardware.camera" android:required="false"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />
    <uses-permission android:name="android.permission.SET_WALLPAPER_HINTS" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/pnp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/pnp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="pandpdesign.wallpapersapp.WallpaperActivity" />
        <activity android:name="pandpdesign.wallpapersapp.WallpaperGalleryActivity" />
        <activity android:name="pandpdesign.wallpapersapp.HelpActivity" />
        <activity android:name="pandpdesign.wallpapersapp.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pandpdesign.wallpapersapp.Menu"
            android:label="@string/title_activity_menu" />
        <activity android:name="pandpdesign.wallpapersapp.WallpaperUpload"></activity>
    </application>

</manifest>